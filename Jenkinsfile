node {
  parameters {
    booleanParam(name: 'executeUITests', defaultValue: true, description: '' )
  }
  stages{
    stage("build"){
      steps{
        echo "build is completed"
      }
    }
    stage("test"){
      steps{
        echo "tests are completed"
      }
    }
    stage("deploy"){
      steps{
        echo "succesful deployed"
      }
    }
  }
}
